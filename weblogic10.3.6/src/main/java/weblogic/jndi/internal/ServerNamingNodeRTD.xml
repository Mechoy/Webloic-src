<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE rmi PUBLIC "-//BEA Systems, Inc.//RMI Runtime DTD 1.0//EN" "rmi.dtd">
<rmi
    name="weblogic.jndi.internal.ServerNamingNode"
    server-ref-classname="weblogic.jndi.internal.AdminRoleBasedDispatchServerRef"
    use-server-side-stubs="true"
>
<cluster
    clusterable="true"
    replica-handler-classname="weblogic.jndi.internal.NamingNodeReplicaHandler"
    propagate-environment="true"
>
</cluster>

<lifecycle
    dgc-policy="managed"
>
</lifecycle>

<method
     name="*"
    transactional="false"
>
</method>

</rmi>
