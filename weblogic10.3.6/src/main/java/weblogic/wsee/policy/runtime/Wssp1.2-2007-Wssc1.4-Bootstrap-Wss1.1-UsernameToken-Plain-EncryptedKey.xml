<?xml version="1.0"?>
<wsp15:Policy xmlns:wsp15="http://www.w3.org/ns/ws-policy"  xmlns:sp="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702">
	<sp:SymmetricBinding>
		<wsp15:Policy>
			<sp:ProtectionToken>
				<wsp15:Policy>
					<sp:SecureConversationToken sp:IncludeToken="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702/IncludeToken/AlwaysToRecipient">
						<wsp15:Policy>
							<sp:RequireDerivedKeys/>
							<sp:BootstrapPolicy>
								<wsp15:Policy>
									<sp:SignedParts>
										<sp:Body/>
										<sp:Header Namespace="http://schemas.xmlsoap.org/ws/2004/08/addressing"/>
										<sp:Header Namespace="http://www.w3.org/2005/08/addressing"/>
									</sp:SignedParts>
									<sp:EncryptedParts>
										<sp:Body/>
									</sp:EncryptedParts>
									<sp:SymmetricBinding>
										<wsp15:Policy>
											<sp:ProtectionToken>
												<wsp15:Policy>
													<sp:X509Token sp:IncludeToken="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702/IncludeToken/AlwaysToRecipient">
														<wsp15:Policy>
															<sp:RequireDerivedKeys/>
															<sp:RequireThumbprintReference/>
															<sp:WssX509V3Token10/>
														</wsp15:Policy>
													</sp:X509Token>
												</wsp15:Policy>
											</sp:ProtectionToken>
											<sp:AlgorithmSuite>
												<wsp15:Policy>
													<sp:Basic256/>
												</wsp15:Policy>
											</sp:AlgorithmSuite>
											<sp:Layout>
												<wsp15:Policy>
													<sp:Lax/>
												</wsp15:Policy>
											</sp:Layout>
											<sp:IncludeTimestamp/>
											<sp:OnlySignEntireHeadersAndBody/>
										</wsp15:Policy>
									</sp:SymmetricBinding>
									<sp:SignedEndorsingSupportingTokens>
										<wsp15:Policy>
											<sp:X509Token sp:IncludeToken="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702/IncludeToken/AlwaysToRecipient">
												<wsp15:Policy>
													<sp:RequireThumbprintReference/>
													<sp:WssX509V3Token10/>
												</wsp15:Policy>
											</sp:X509Token>
										</wsp15:Policy>
									</sp:SignedEndorsingSupportingTokens>
									<sp:SignedEncryptedSupportingTokens>
										<wsp15:Policy>
											<sp:UsernameToken sp:IncludeToken="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702/IncludeToken/AlwaysToRecipient">
												<wsp15:Policy>
													<sp:WssUsernameToken11/>
												</wsp15:Policy>
											</sp:UsernameToken>
										</wsp15:Policy>
									</sp:SignedEncryptedSupportingTokens>
									<sp:Wss11>
										<wsp15:Policy>
											<sp:MustSupportRefKeyIdentifier/>
											<sp:MustSupportRefIssuerSerial/>
											<sp:MustSupportRefThumbprint/>
											<sp:MustSupportRefEncryptedKey/>
											<sp:RequireSignatureConfirmation/>
										</wsp15:Policy>
									</sp:Wss11>
								</wsp15:Policy>
							</sp:BootstrapPolicy>
						</wsp15:Policy>
					</sp:SecureConversationToken>
				</wsp15:Policy>
			</sp:ProtectionToken>
			<sp:AlgorithmSuite>
				<wsp15:Policy>
					<sp:Basic256/>
				</wsp15:Policy>
			</sp:AlgorithmSuite>
			<sp:Layout>
				<wsp15:Policy>
					<sp:Lax/>
				</wsp15:Policy>
			</sp:Layout>
			<sp:IncludeTimestamp/>
			<sp:OnlySignEntireHeadersAndBody/>
		</wsp15:Policy>
	</sp:SymmetricBinding>
	<sp:Wss11>
		<wsp15:Policy>
			<sp:MustSupportRefKeyIdentifier/>
			<sp:MustSupportRefIssuerSerial/>
			<sp:MustSupportRefThumbprint/>
			<sp:MustSupportRefEncryptedKey/>
			<sp:RequireSignatureConfirmation/>
		</wsp15:Policy>
	</sp:Wss11>
	<sp:Trust13>
		<wsp15:Policy>
			<sp:MustSupportIssuedTokens/>
			<sp:RequireClientEntropy/>
			<sp:RequireServerEntropy/>
		</wsp15:Policy>
	</sp:Trust13>
</wsp15:Policy>
